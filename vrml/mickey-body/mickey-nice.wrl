Inline {
	url "mickey-static.wrl"
}

Background {
	skyColor [ 0.95 0.84 0.26 ]
	frontUrl "parts/mickey-background2.png"
}


#Linkes Auge
Transform {
	scale 0.65 0.9 0.2
	scaleOrientation 0 0 1 0 
	center 0 0 0 
	rotation 1 1 0 -0.4
	translation -0.5 0.6 1.67
	bboxCenter 0 0 0
	bboxSize -1 -1 -1
	children [
		DEF WhiteSphere Shape {
			appearance Appearance {
				material Material {
					diffuseColor 1 1 1
					specularColor 0 0 0 
					shininess 0
					emissiveColor 0.3 0.3 0.3
				}
			}
			geometry Sphere {
				#size 2.0 2.0 2.0
				radius 1.0
			}	
		}
	]
}

#Rechtes Auge
Transform {
	scale 0.65 0.9 0.2
	scaleOrientation 0 0 1 0 
	center 0 0 0 
	rotation 1 -1 0 -0.4
	translation 0.5 0.6 1.67
	bboxCenter 0 0 0
	bboxSize -1 -1 -1
	children [
		USE WhiteSphere
	]
}

#Linke Pupille
Transform {
	scale 0.1 0.2 0.02
	scaleOrientation 0 0 1 0 
	center 0 0 0 
	rotation 1 0.5 0 -0.15
	translation -0.32 0.2 2.0
	bboxCenter 0 0 0
	bboxSize -1 -1 -1
	children [
		DEF BlackSphere Shape {
			appearance Appearance {
				material Material {
					diffuseColor 0 0 0
				}
			}
			geometry Sphere {
				#size 2.0 2.0 2.0
				radius 2.0
			}	
		}
	]
}

#Rechte Pupille
Transform {
	scale 0.1 0.2 0.02
	scaleOrientation 0 0 1 0 
	center 0 0 0 
	rotation 1 -0.5 0 -0.15
	translation 0.32 0.2 2.0
	bboxCenter 0 0 0
	bboxSize -1 -1 -1
	children [
		USE BlackSphere
	]
}


